package recapDemo1;

public class Main {

	public static void main(String[] args) {
		
		int sayi1 = 30;
		int sayi2 = 25;
		int sayi3 =42;
		
		if (sayi1 > sayi2 && sayi2 > sayi3) {
			System.out.println("En büyük sayi : " + sayi1);
		}else if (sayi1 > sayi3 && sayi3 > sayi2) {
			System.out.println("En büyük sayi : " + sayi1);
		}else if (sayi2 > sayi1 && sayi1 > sayi3) {
			System.out.println("En büyük sayi : " + sayi2);
		}else if (sayi2 > sayi3 && sayi3 > sayi1) {
			System.out.println("En büyük sayi : " + sayi2);
		}else {
			System.out.println("En büyük sayi : " + sayi3);
		}

	}

}
