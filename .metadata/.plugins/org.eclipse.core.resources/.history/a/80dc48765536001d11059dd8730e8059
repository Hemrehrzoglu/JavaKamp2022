package miniProjeAsalSayi;

public class Main {

	public static void main(String[] args) {

		int sayi = 29;
		
		String asalMi = "";
		
		for (int i = 2; i < sayi; i++) {
			
			if(sayi% i == 0  || sayi%i ==1) {
				
				asalMi = "sayı asal değildir";
				System.out.println("s");
			}else {
				asalMi = "sayı asaldır";
				System.out.println("c");
			}
			
			
		}
		System.out.println(asalMi);

		
	}

}
