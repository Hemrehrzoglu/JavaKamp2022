package miniProjeAsalSayi;

public class Main {

	public static void main(String[] args) {

		int sayi = 9;
		String asalMi="";

		for (int i = 2; i < sayi; i++) {
			
			int a = sayi/i;
			
			if(a%2 != 0 && a!=1) {
				System.out.println(a + " sayısı asaldır");
				
			}else {
				System.out.println(a + " sayısı asal değildir");
			}

		}
	}

}
