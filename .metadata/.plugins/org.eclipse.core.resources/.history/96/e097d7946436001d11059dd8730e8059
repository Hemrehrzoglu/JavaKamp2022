package sayiBulma;

public class Main {

	public static void main(String[] args) {
		int [] sayilar = {1,2,5,7,9,0};
		int aranacak = 5;
		
		for(int ara : sayilar) {
			
			if(ara == aranacak) {
				System.out.println("Buldunuz");
			}
			else {
				System.out.println("Malesef bulamadınız.");
			}
		}

	}

}
