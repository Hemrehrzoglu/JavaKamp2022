package arraysDemo;

public class Main {

	public static void main(String[] args) {
		
		String [] takımlar = new String [5];
		takımlar[0] = "Galatasaray" ;
		takımlar[1] = "Fenerbahçe" ;
		takımlar[2] = "Beşiktaş" ;
		takımlar[3] = "Trabzon" ;
		takımlar[4] = "Sakarya" ;
		
		for (int i = 0; i < takımlar.length; i++) {
			System.out.println("For : " +takımlar[i]);
		}
		
		for(String takım : takımlar) {
			System.out.println("forin : " + takım);
		}

	}

}
