package switchDemo;

public class Main {

	public static void main(String[] args) {
		
		String marka = "Volvo";
		
		switch (marka) {
		case "Bmw":
			System.out.println("Arabanızın markası : " + marka);
			break;
		case "Mercedes" :
			System.out.println("Arabanızın markası : " + marka);
		case "Volvo" :
			System.out.println("Arabanızın markası : " + marka);

		default:
			System.out.println("Bilinmeyen marka");
			break;
		}
	}

}
