package miniProjeAsalSayi;

public class Main {

	public static void main(String[] args) {

		int sayi = 9;

		for (int i = 2; i < sayi; i++) {
			if(sayi % i != 0 && sayi % i != 1 && sayi % sayi ==0) {
				System.out.println(sayi + " sayısı asaldır");
			}else {
				System.out.println(sayi + " sayısı asal değildir");
			}

		}
	}

}
