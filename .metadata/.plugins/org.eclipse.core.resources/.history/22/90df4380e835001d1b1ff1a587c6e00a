package switchDemo;

public class Main {

	public static void main(String[] args) {

		String marka = "Bmw";

		switch (marka) {
		case "Bmw":
			System.out.println("Arabanızın markası : " + marka);
			break;
		case "Mercedes":
			System.out.println("Arabanızın markası : " + marka);
			break;
		case "Volvo":
			System.out.println("Arabanızın markası : " + marka);
			break;

		default:
			System.out.println("Bilinmeyen marka");
			break;
		}
	}

}
